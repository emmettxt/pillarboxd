{
  "name": "backend",
  "version": "1.0.0",
  "description": "express backend for Pillarboxd app",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --verbose --forceExit --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --verbose --forceExit --watch",
    "start:server": "cross-env NODE_ENV=production nodemon server/index.js",
    "start:client": "cd ./client && react-scripts start HOST=0.0.0.0",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "build:ui": "rm -rf build && cd ../frontend && npm run build && cp -r build ../backend"
  },
  "author": "Emmet Moore",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-random-string": "^5.0.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.2",
    "nodemailer": "^6.8.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "jest": "^27.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-show-more-text": "^1.6.2",
    "react-swipeable-views": "^0.14.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/jest": "^29.1.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.1.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  },
  "jest": {
    "verbose": true
  }
}