{
  "name": "pillarboxd",
  "version": "1.0.0",
  "description": "MERN stack web app for reviewing TV shows",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --verbose --forceExit --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --verbose --forceExit --watch",
    "start": "node server/index.js",
    "start:dev": "concurrently --kill-others 'npm run start:server' -k 'npm run start:client'",
    "start:server": "cross-env NODE_ENV=production nodemon server/index.js",
    "start:client": "cd ./client && npm start",
    "build": "cd ./client && npm run build",
    "build:azure": "cd ./client && npm run build",
    "postinstall": "cd ./client && npm install"
  },
  "author": "Emmet Moore",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-random-string": "^5.0.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.1.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  },
  "jest": {
    "verbose": true
  }
}